{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,jpeg,gif,svg,ico,webp}", "/css/*", "/js/*", "/assets/*"]
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
